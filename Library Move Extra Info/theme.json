{
  "name": "Library Move Extra Info",
  "author": "Matthias#8282",
  "manifest_version": 3,
  "target": "Library",
  "description": "This is a theme that moves the extra info on top of the banner.",
  "inject": {
      "shared.css": ["SP", "MainMenu", "QuickAccess"]
  },
  "patches": {
        "Start With Margin": {
            "type": "checkbox",
            "values": {
                "No": {},
                "Yes": {
                    "startmargin.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        }
  }
}
