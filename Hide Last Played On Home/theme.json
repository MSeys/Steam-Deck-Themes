{
  "name": "Hide Last Played On Home",
  "author": "Matthias#8282",
  "manifest_version": 3,
  "target": "Tweak",
  "description": "This is a theme that allows you to hide the extra information on home screen.",
  "inject": {
      "shared.css": ["SP", "MainMenu", "QuickAccess"]
  },
  "patches": {
        "Add Margin": {
            "type": "checkbox",
            "values": {
                "No": {},
                "Yes": {
                    "margin.css": ["SP", "MainMenu", "QuickAccess"]
                }
            }
        }
  }
}
